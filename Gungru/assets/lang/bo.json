    "a_what_is_buddhism": "བོད་ཀྱི་ནང་ཆོས་ནི་བོད་དང་ཧི་མ་ལ་ཡའི་ས་ཁུལ་ཁག་ཏུ་དར་རྒྱས་བྱུང་བའི་ཐེག་ཆེན་ནང་ཆོས་ཀྱི་རྣམ་པ་ཞིག་ཡིན། འདིས་སྟོན་པའི་བཀའ་ཆོས་དང་སྔགས་ཀྱི་ཉམས་ལེན། གཞུང་ལུགས་སློབ་གཉེར། མོས་གུས་ཀྱི་ཆོ་ག་བཅས་མཉམ་སྦྲེལ་བྱས་ཡོད། སེམས་ཅན་ཐམས་ཅད་ལ་སྙིང་རྗེ་དང་ཤེས་རབ་སྒྲུབ་པ། གཞན་ཕན་གྱི་ཆེད་བྱང་ཆུབ་ཐོབ་པའི་སྨོན་ལམ་བཅས་ལ་གཙིགས་འཛིན་བྱེད།",
    "q_gelug": "དགེ་ལུགས་ཀྱི་ལམ་སྲོལ་གང་ཡིན་ནམ།",
    "a_gelug": "དགེ་ལུགས་ཆོས་ལུགས་ནི་ཕྱི་ལོ་ ༡༥ པའི་ལོ་རབས་ཀྱི་མགོ་ཙམ་ལ་རྗེ་ཙོང་ཁ་པས་བཙུགས་པ་ཡིན། འདི་ནི་བོད་ཀྱི་ནང་ཆོས་ཀྱི་ཆོས་ལུགས་ཆེན་པོ་བཞིའི་ཡ་གྱལ་ཞིག་ཡིན། གཞུང་ལུགས་སློབ་གཉེར་ཟབ་རྒྱས་དང་གྲྭ་ཚང་གི་བཅའ་ཁྲིམས། རིམ་གྱིས་བྱང་ཆུབ་ཀྱི་ལམ་བཅས་ལ་གཙིགས་འཛིན་བྱེད། དགའ་ལྡན་དགོན་པ་ཕྱི་ལོ་ ༡༤༠༩ ལ་ཙོང་ཁ་པ་རང་གིས་བཙུགས་པ་ཡིན་ཞིང་དགེ་ལུགས་ཀྱི་ལམ་སྲོལ་གྱི་དགོན་པ་གཙོ་བོ་ཡིན། སྤྱི་ནོར་གོང་ས་སྐྱབས་མགོན་ཆེན་པོ་མཆོག་ནི་དགེ་ལུགས་ཀྱི་བརྒྱུད་པའི་སྟོན་པ་ཁྱད་དུ་འཕགས་པ་ཡིན།",
    "q_khangtsen": "ཁང་ཚན་ཅི་ཡིན་ནམ།",
    "a_khangtsen": "ཁང་ཚན་ཞེས་པ་ནི་བོད་ཀྱི་དགོན་པ་ཆེན་པོའི་ནང་གི་ས་ཁུལ་གྱི་ཁང་པ་ཞིག་ཡིན། གྲྭ་བཙུན་ཚོ་བོད་ནང་གི་ས་ཁུལ་ལ་གཞིགས་ནས་ཁང་ཚན་ཚོར་བགོས་ཡོད། གུང་རུ་ཁང་ཚན་ནི་དགའ་ལྡན་ཤར་རྩེ་དགོན་པའི་ནང་གི་ཁང་ཚན་བཅུ་གཅིག་གི་ཡ་གྱལ་ཞིག་ཡིན། ཁང་ཚན་རེ་རེས་གྲྭ་བཙུན་ཚོར་གནས་སྡོད་དང་ཟས། ཚོགས་པའི་རོགས་རམ་བཅས་མཁོ་སྤྲོད་བྱས་ཏེ། གྲྭ་ཚང་ཆེན་པོའི་ནང་གི་ཁྱིམ་ཚང་མཉམ་འབྲེལ་ཞིག་གི་ལས་ཀ་བྱེད་ཀྱི་ཡོད།",
    "q_four_truths": "བདེན་པ་བཞི་གང་ཡིན་ནམ།",
    "a_four_truths": "བདེན་པ་བཞི་ནི་སངས་རྒྱས་ཀྱིས་བྱང་ཆུབ་ཐོབ་རྗེས་དང་པོ་གསུངས་པའི་ནང་ཆོས་ཀྱི་རྨང་གཞིའི་བཀའ་ཆོས་ཡིན། དེ་དག་ནི། (༡) སྡུག་བསྔལ་གྱི་བདེན་པ། &mdash; མི་ཚེ་ལ་མི་དགའ་བ་མེད་ཐབས་མེད་ཡོད་པ། (༢) ཀུན་འབྱུང་གི་བདེན་པ། &mdash; སྡུག་བསྔལ་སྲེད་པ་དང་མ་རིག་པ་ལས་འབྱུང་བ། (༣) འགོག་པའི་བདེན་པ། &mdash; སྡུག་བསྔལ་མཇུག་སྒྲིལ་ཐུབ་པ། དང་ (༤) ལམ་གྱི་བདེན་པ། &mdash; སྡུག་བསྔལ་མཇུག་སྒྲིལ་བའི་ལམ་ཕྱག་ལེན་ཅན་ཞིག་ཡོད་པ། དེ་ལ་འཕགས་ལམ་ཡན་ལག་བརྒྱད་ཅེས་གྲགས།",
    "group_pujas": "མཆོད་པ་དང་གསོལ་འདེབས་སྐོར།",
    "q_what_is_puja": "མཆོད་པ་གང་ཡིན་ནམ།",
    "a_what_is_puja": "མཆོད་པ་ནི་གྲྭ་བཙུན་ཚོས་སྒྲུབ་པའི་ནང་ཆོས་ཀྱི་གསོལ་འདེབས་ཀྱི་ཆོ་ག་ཡིན། འདིར་དམ་པའི་གཞུང་ཀློག་འདོན་དང་མཆོད་པ་ཕུལ་བ། སྨན་བཅོས་དང་སྲུང་སྐྱོབ། སྦྱང་བ། བསོད་ནམས་བསགས་པ་བཅས་ཀྱི་དམིགས་ཡུལ་ཁ་ཕྱོགས་ཏེ་ཆོ་ག་སྒྲུབ་པ་བཅས་ཚུད་ཡོད། མི་སྒེར་གྱིས་རང་ཉིད་ཡང་ན་གཞན་གྱི་ཆེད་དུ། ཚེ་ལས་འདས་པའི་མཛའ་བརྩེའི་མི་ཚོའི་ཆེད་ཀྱང་མཆོད་པ་ཞུ་ཆོག",
    "q_puja_types": "མཆོད་པའི་རིགས་གང་དག་ཞུ་ཆོག་གམ།",
    "a_puja_types": "ང་ཚོས་མཆོད་པའི་རིགས་མང་པོ་མཁོ་སྤྲོད་བྱེད་ཀྱི་ཡོད། <strong>སྨན་བླའི་མཆོད་པ་</strong> སྨན་བཅོས་དང་བདེ་ཐང་གི་ཆེད། <strong>སྒྲོལ་མའི་མཆོད་པ་</strong> སྲུང་སྐྱོབ་དང་བར་ཆད་སེལ་བའི་ཆེད། <strong>འཇམ་དབྱངས་ཀྱི་མཆོད་པ་</strong> ཤེས་རབ་དང་སློབ་གཉེར་ལེགས་འགྲུབ་ཀྱི་ཆེད། <strong>བླ་མ་མཆོད་པ་</strong> ཆོས་ཀྱི་འཕེལ་རྒྱས་ཀྱི་ཆེད། དང་ <strong>ཚེ་ལས་འདས་པ་ཚོའི་ཆེད་ཀྱི་དམིགས་བསལ་གསོལ་འདེབས་</strong> ཡང་སྲིད་བཟང་པོར་ཁྲིད་པའི་ཆེད་བཅས་ཡོད། ཁྱེད་ཀྱི་མཁོ་བ་ལ་མཆོད་པ་གང་འཚམ་པ་གླེང་མོལ་བྱ་རྒྱུར་ང་ཚོར་འབྲེལ་བ་གནང་རོགས།",
    "q_request_puja": "མཆོད་པ་ཇི་ལྟར་ཞུ་དགོས་སམ།",
    "a_request_puja": "ང་ཚོའི་དྲ་ཚིགས་ཐོག་ནས་ཡང་ན་ <strong>info@gadengungru.com</strong> གློག་འཕྲིན་ཐོག་ནས། ཡང་ན་ང་ཚོའི་ཨ་མེ་རི་ཀའི་འཁོར་བསྐྱོད་སྐབས་འབྲེལ་བ་བྱས་ནས་མཆོད་པ་ཞུ་ཆོག ང་ཚོས་ཁྱེད་ཀྱི་མཁོ་བ་དང་དམིགས་ཡུལ་གླེང་མོལ་བྱས་ཏེ། རྒྱ་གར་ཡོད་པའི་དགོན་པར་གྲྭ་བཙུན་ཚོས་ཆོ་ག་སྒྲུབ་རྒྱུ་ཡིན། མཆོད་པ་ཚར་རྗེས་ཁྱེད་ལ་གཏན་འབེབས་དང་བྱིན་རླབས་འབྱོར་རྒྱུ་ཡིན།",
    "q_butter_lamp": "མར་མེ་མཆོད་པ་གང་ཡིན་ནམ།",
    "a_butter_lamp": "མར་མེ་ནི་བོད་ཀྱི་ནང་ཆོས་ཀྱི་སྲོལ་རྒྱུན་གྱི་མཆོད་པ་ཞིག་ཡིན། མར་མེའི་སྣང་བ་ནི་མ་རིག་པའི་མུན་པ་སེལ་བའི་ཤེས་རབ་མཚོན་ཀྱི་ཡོད། ཁྱེད་ཀྱི་མིང་གམ་ཡང་ན་མཛའ་བརྩེའི་མི་ཞིག་གི་རྗེས་དྲན་དུ་མར་མེ་ཕུལ་ཆོག འདི་ནི་ཁྱེད་ཀྱིས་ཕུལ་ཆོག་པའི་མཆོད་པ་ཁྲོད་ཀྱི་སྟབས་བདེ་ཤོས་དང་དོན་ཆེ་ཤོས་ཤིག་ཡིན་ཞིང། ཞལ་འདེབས་ཆུང་ཆུང་ &#8377;108 (~$1.30 USD) ཙམ་གྱིས་ཐོབ་ཆོག",
